{
  "entrypoint": "index.html",
  "shell": "src/my-app.html",
  "fragments": [
    "src/ivd-map.html",
    "src/ivd-highscore.html",
    "src/ivd-cities.html",
    "src/ivd-404.html"
  ],
  "sources": [
    "src/**/*",
    "locations.json",
    "cities.json",
    "mapStyles.json",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "images/**/*",
    "!images/invaders/*",
    "!images/invaders/",
    "!images/books/",
    "!images/maps/",
    "fonts/*",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "name": "fallback",
      "addServiceWorker": true,
      "addPushManifest": true,
      "swPrecacheConfig": "sw-precache-config.js",
      "insertPrefetchLinks": true,
      "bundle": {
        "inlineCss": true,
        "inlineScript": true,
        "rewriteUrlsInTemplates": false,
        "sourcemaps": true,
        "stripComments": true
      },
      "html": {
        "minify": true
      },
      "css": {
        "minify": true
      },
      "js": {
        "minify": true,
        "compile": true
      },
      "basePath": true
    },
    {
      "name": "modern",
      "addServiceWorker": true,
      "addPushManifest": true,
      "swPrecacheConfig": "sw-precache-config.js",
      "insertPrefetchLinks": true,
      "bundle": {
        "inlineCss": true,
        "inlineScript": true,
        "rewriteUrlsInTemplates": false,
        "sourcemaps": true,
        "stripComments": true
      },
      "html": {
        "minify": true
      },
      "css": {
        "minify": true
      },
      "js": {
        "minify": true,
        "compile": true
      },
      "browserCapabilities": ["es2015", "push", "serviceworker"],
      "basePath": true
    }
  ]
}
